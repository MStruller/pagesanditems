body {
	margin:5px !important;
}
.contentcontainer
{
	float: left;
	height: 320px;
	overflow-x: auto;
	overflow-y: auto;
	padding-bottom: 0;
	padding-left: 0;
	padding-right: 0;
	padding-top: 0;
	width: 100%;
}


h2.modal-title 
{
	margin-left: 15px;
	margin-bottom: 0;
	margin-top: 5px;
}

ul.menu_types 
{
	padding: 0 0 0 15px;
	width: 95%;
	margin: 0;
}

ul.menu_types li 
{
	width: 46% !important;
}

ul.menu_types li,
dl.menu_type dd ul li 
{
	width: 240px !important;
	list-style: none;
	display: block;
	float: left;
	margin-right: 10px;
}



dl.menu_type
{
	width: 240px;
	margin: 0;
	padding: 0;
}

dl.menu_type dt {
	font-weight: bold;
	font-size: 1.091em;
	float: left;
	margin: 13px 0 5px 0;
	border-bottom: 1px solid #666;
	width: 240px;
}

dl.menu_type dd {
	clear: left;
	margin: 0;
}

dl.menu_type dd ul li {
	margin: 0;
}

dl.menu_type dd ul 
{
	padding-start-value: 0 !important;
	margin-left: 0 !important;
}

