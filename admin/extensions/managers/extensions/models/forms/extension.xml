<?xml version="1.0" encoding="utf-8"?>
	<!-- $Id: plugin.xml 19203 2010-10-22 16:28:34Z chdemko $ -->
<form>
	<fieldset>
		<field
			name="extension_id"
			label="JGLOBAL_FIELD_ID_LABEL"
			description ="JGLOBAL_FIELD_ID_DESC"
			type="text"
			default="0"
			readonly="true"
			class="readonly" />

		<field
			name="version"
			label="COM_PAGESANDITEMS_VERSION"
			type="text"
			default="0"
			readonly="true"
			class="readonly" />

		<field
			name="name"
			type="text"
			readonly="true"
			class="readonly"
			label="COM_PAGESANDITEMS_NAME"
		 />

		<field
			name="enabled"
			type="list"
			label="JENABLED"
			class="inputbox"
			size="1"
			default="1">
			<option
				value="1">
				JENABLED</option>
			<option
				value="0">
				JDISABLED</option>
		</field>

		<field
			name="folder"
			type="text"
			class="readonly"
			size="20"
			label="COM_PAGESANDITEMS_EXTENSIONS_FOLDER"
			readonly="true" />

		<field
			name="type"
			type="text"
			class="readonly"
			size="20"
			label="COM_PAGESANDITEMS_EXTENSIONS_TYPE"
			readonly="true" />

		<field
			name="element"
			type="text"
			class="readonly"
			size="20"
			label="COM_PAGESANDITEMS_ELEMENT"
			readonly="true" />

		<!-- type="accesslevel" -->
		<field
			name="access"
			type="hidden"
			label="JFIELD_ACCESS_LABEL"
			description="JFIELD_ACCESS_DESC"
			class="inputbox"
			size="1" />

		<!--type="ordering"-->
		<field
			name="ordering"
			type="hidden"
			class="inputbox"
			label="JFIELD_ORDERING_LABEL"
			description="JFIELD_ORDERING_DESC" />

		<!-- type="list" -->
		<field name="state" type="hidden" label="JGLOBAL_STATE"
			description="JFIELD_PUBLISHED_DESC" class="inputbox"
			filter="intval" size="1" default="1"
		>
			<option value="1">
				JPUBLISHED</option>
			<option value="0">
				JUNPUBLISHED</option>
			<option value="-1">
				JARCHIVED</option>
			<option value="-2">
				JTRASHED</option>
		</field>
	</fieldset>
</form>
